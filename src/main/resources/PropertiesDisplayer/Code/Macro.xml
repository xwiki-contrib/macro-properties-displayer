    <property>
      <code>{{velocity wiki="true"}}&#xd;
#set ($object = $wikimacro.parameters.object)&#xd;
#set ($propertiesList = $wikimacro.parameters.propertiesList)&#xd;
#set ($displayEmptyValues = $wikimacro.parameters.displayEmptyValues)&#xd;
#set ($blockNoColumns = $wikimacro.parameters.blockClasses)&#xd;
#set ($propertyNameNoColumns = $wikimacro.parameters.propertyNameClasses)&#xd;
#set ($propertyValuesNoColumns = $wikimacro.parameters.propertyValuesClasses)&#xd;
#set ($blockStyle = $wikimacro.parameters.styles)&#xd;
#set ($blockTitle = $wikimacro.parameters.blockTitle)&#xd;
#set ($blockBgColor = $wikimacro.parameters.blockBackgroundColor)&#xd;
&#xd;
#if ('' != "$!blockBgColor")&#xd;
  #if ($objecttool.isNull($blockStyle))&#xd;
    #set ($blockStyle = '')&#xd;
  #end&#xd;
    #set($blockStyle = $blockStyle + " background: $blockBgColor;")&#xd;
#end&#xd;
&#xd;
## The list of properties reffers to the current object.&#xd;
#if ($objecttool.isNotNull($object))&#xd;
  #set ($discard = $doc.use($object))&#xd;
  #if ($objecttool.isNotNull($propertiesList))&#xd;
    {{html wiki="true"}}&#xd;
&#xd;
    ## Block size here&#xd;
    (% class="$blockNoColumns" style="$blockStyle" %)&#xd;
    (((&#xd;
    (% class=row% style="text-align:center" %) ((($blockTitle)))&#xd;
    #foreach($prop in $propertiesList)&#xd;
      ## By default, display empty properties.&#xd;
      #if ('true' == $displayEmptyValues || ('false' == $displayEmptyValues &amp;&amp; '' != $doc.display($prop)))&#xd;
        (% class=row %)&#xd;
        (((&#xd;
        (% class="$propertyNameNoColumns" %)((( $doc.displayPrettyName($prop) )))&#xd;
        (% class="$propertyValuesNoColumns" %)((( $doc.display($prop) )))&#xd;
        )))&#xd;
      #end&#xd;
    #end&#xd;
    )))&#xd;
&#xd;
    {{/html}}&#xd;
&#xd;
    #end&#xd;
#end&#xd;
&#xd;
{{/velocity}}</code>
