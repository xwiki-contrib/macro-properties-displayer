    <property>
      <code>{{velocity wiki="true"}}
## wikimacro.parameters is available from XWiki 10.11.9, 11.3.2 and 11.6RC1
#if ("$!wikimacro.parameters" == '')
  #set ($wikimacro = {})
  #set ($wikimacro.parameters = $xcontext.macro.params)
  ## $propertiesList should be a list, since the parameters are strings.
  #set ($propertiesList = $wikimacro.parameters.properties.split(", "))
#else
  #set ($propertiesList = $wikimacro.parameters.properties)
#end
##
#set ($object = $doc.getObject($wikimacro.parameters.className))
## $propertiesList is by default equal to all parameters
#if ("$!propertiesList" == '')
  #set ($propertiesList = $collectionstool.set)
  #set ($objectProperties = $object.getProperties())
  #foreach ($prop in $objectProperties)
    #set ($discard = $propertiesList.add($prop.getName()))
  #end
#end
#set ($displayEmptyValues = $wikimacro.parameters.displayEmptyValues)
#set ($blockClasses = $wikimacro.parameters.blockClasses)
#set ($propertyNameClasses = $wikimacro.parameters.propertyNameClasses)
#set ($propertyValuesClasses = $wikimacro.parameters.propertyValuesClasses)
#set ($blockStyle = $wikimacro.parameters.style)
#set ($blockBgColor = $wikimacro.parameters.backgroundColor)
#set ($title = $wikimacro.parameters.title)
#set ($titleStyle = $wikimacro.parameters.titleStyle)
#set ($titleClasses = $wikimacro.parameters.titleClasses)
#if ("$!blockBgColor" != '')
  #if ("$!blockStyle" == '')
    #set ($blockStyle = '')
  #end
    #set($blockStyle = $blockStyle + " background: $blockBgColor;")
#end
##
#if ("$!object" != '')
  #set ($discard = $doc.use($object))
  #if ("$!propertiesList" != '')

    {{html wiki="true"}}

    ## Block size
    (% class="$blockClasses" style="$blockStyle" %)
    (((
    (% class=row %)
    (((
    (% class="$titleClasses" style="$!titleStyle" %)
      ((($!title)))
    )))
    #foreach($prop in $propertiesList)
      ## By default, display empty properties.
      #if ($displayEmptyValues == 'true' || ($displayEmptyValues == 'false' &amp;&amp; $doc.display($prop) != ''))
        (% class=row %)
        (((
        (% class="$propertyNameClasses" %)((( $doc.displayPrettyName($prop) )))
        (% class="$propertyValuesClasses" %)((( $doc.display($prop) )))
        )))
      #end
    #end
    )))

    {{/html}}

    #end
#end
{{/velocity}}</code>
