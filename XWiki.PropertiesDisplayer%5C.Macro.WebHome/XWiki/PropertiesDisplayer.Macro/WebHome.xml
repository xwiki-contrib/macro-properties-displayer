      <code>{{velocity}}&#xd;
#set ($object = $wikimacro.parameters.object)&#xd;
#set ($propertiesList = $wikimacro.parameters.propertiesList)&#xd;
#set ($displayEmptyValues = $wikimacro.parameters.displayEmptyValues)&#xd;
&#xd;
{{html wiki="true"}}&#xd;
&#xd;
  ## By default, display empty properties.&#xd;
  #if ($objecttool.isNull( $displayEmptyValues))&#xd;
    #set ($displayEmptyValues = true)&#xd;
  #end&#xd;
&#xd;
  ## The list of properties reffers to the current object.&#xd;
  #if ($objecttool.isNotNull($object))&#xd;
    #set ($discard = $doc.use($object))&#xd;
    #if ($objecttool.isNotNull($propertiesList))&#xd;
      ## TODO: each property in a separated row.&#xd;
      #foreach($prop in $propertiesList)&#xd;
        $prop&#xd;
        $doc.display($prop)&#xd;
      #end&#xd;
    #end&#xd;
  #end&#xd;
&#xd;
{{/html}}&#xd;
&#xd;
{{/velocity}}</code>
